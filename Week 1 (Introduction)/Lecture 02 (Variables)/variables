### your name, wher u are living, two movies u like, and ur favourite baskteball player
## using one line or multiple, if using multiple call the function each line
print("\n\n\nHard-Coded\n\n\n")
print("MY INFROMATION: ")
print("My name is Rohan","I live in Mississauga")
print("MY FAVOURITES: ")
print("my favourite movie is Inception and my second favourite movie is the Matrix","and my favourite basketball player is Lebron James")

print("\n\n\nWith Variables\n\n\n")
myName = "Rohan"
myCity = "Mississauga"
myFirstMovie = "Inception"
mySecondMovie = "The Matrix"
myFavBBAll = "Lebron James"
print("MY INFORMATION:")
print("My name is",myName,"I live in",myCity)
print("MY FAVOURITES: ")
print("my favourite movie is ",myFirstMovie,"and my second favourite movie is",mySecondMovie,"and my favourite basketball player is",myFavBBAll)


print("\n\n\nWith Changed Variables\n\n\n")
myName = "Shaan"
myCity = "the best city"
myFirstMovie = "Ex-Machina"
mySecondMovie = "Inception"
myFavBBAll = "Jordan"
print("MY INFORMATION:")
print("My name is",myName,"I live in",myCity)
print("MY FAVOURITES: ")
print("my favourite movie is ",myFirstMovie,"and my second favourite movie is",mySecondMovie,"and my favourite basketball player is",myFavBBAll)





